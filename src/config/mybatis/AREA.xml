<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qysoft.platform.mappers.AreaMapper">

    <select id="queryAreaList" resultType="com.qysoft.rapid.domain.Bean">
        select * from top_area
    </select>

    <select id="queryById" parameterType="map" resultType="bean" >
        select * from top_area where id = #{id}
    </select>
</mapper>